# Copy this to .env and fill in your values

# Django settings
DJANGO_SECRET_KEY=your_secret_key_here
DEBUG=False
ALLOWED_HOSTS=your-app.onrender.com,sellerprep.app

# Database (Render provides these automatically)
DATABASE_URL=postgresql://user:pass@host:port/dbname

# Email service (Namecheap Private Email)
EMAIL_HOST_USER=support@sellerprep.app
EMAIL_HOST_PASSWORD=your_email_password_here
DEFAULT_FROM_EMAIL=no-reply@sellerprep.app

# Alternative: SendGrid (if you prefer)
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Stripe keys  
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here